{
    "editor.fontFamily": "Fira Code",
    "editor.minimap.enabled": false,
    "editor.fontLigatures": true,
    "workbench.productIconTheme": "fluent-icons",
    "workbench.iconTheme": "material-icon-theme",  
    "material-icon-theme.activeIconPack": "angular_ngrx",
    "material-icon-theme.folders.theme": "classic",
    "material-icon-theme.hidesExplorerArrows": true,
    "material-icon-theme.rootFolders.color": "#ef5350",
    "material-icon-theme.opacity": 1,
    "editor.wordWrap": "on",
    //Positron Assistant
    "positron.assistant.enable": true,
    "positron.assistant.enabledProviders": [
        "anthropic"
    ],
    //stataRun
    "stataRun.skipComments": true,
    "stataRun.pasteSpeed": 1,
    "stataRun.stataPath": "C:\\Program Files\\Stata17\\StataSE-64.exe",
    "security.allowedUNCHosts": [
        "orebroll.se"
    ],
    "r.rpath.windows": " \"C:/PROGRA~1/R/R-44~1.2/bin/x64\"",
    "security.workspace.trust.banner": "never",
    "terminal.integrated.defaultProfile.windows": "Git Bash",

    "editor.rulers": [
        80,
        100
    ],
    "[r]": {
        "editor.formatOnSave": true,
        "editor.tabSize": 2
    },

    "diffEditor.hideUnchangedRegions.enabled": true,
    "diffEditor.useInlineViewWhenSpaceIsLimited": false,
    "gitlens.launchpad.indicator.enabled": false,
    "workbench.statusBar.visible": true,

    "terminal.integrated.ignoreProcessNames": [
        "starship",
        "oh-my-posh",
        "bash",
        "zsh",
        "ssh"
    ],
    "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font Mono, Cascadia Code, monospace",
    "terminal.integrated.fontSize": 12, 
    "terminal.integrated.lineHeight": 1.2,
    "workbench.settings.applyToAllProfiles": [
        "terminal.integrated.fontSize"
    ],
    "terminal.integrated.fontWeight": "normal",
    "terminal.integrated.enableVisualBell": true,
    "terminal.integrated.mouseWheelZoom": true,
    "workbench.activityBar.location": "hidden",
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "editor.fontSize": 13,
    "editor.lineHeight": 1.5,
    "editor.cursorBlinking": "smooth",
    "[typst]": {
        "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?"
    },
    "[typst-code]": {
        "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?"
    },
    "quarto.assist.updateMode": "live",

    "markdown-preview-enhanced.enableExtendedTableSyntax": true,
    "markdown-preview-enhanced.wikiLinkTargetFileExtension": ".md, .qmd",

    "markdown-preview-enhanced.pandocArguments": [
       "--filter=pandoc-crossref"
    ],
    "markdown.styles": [
        "https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css"
    ],
    "markdown-preview-enhanced.mathRenderingOption": "MathJax",
  //This for call-outs does not work together with cross-refs
    "markdown-preview-enhanced.usePandocParser": true,

  "cSpell.language": "en-GB",
    //Toggle settings
  "formattingToggle.affects": [
    "cSpell.enabled",
    ],
  "formattingToggle.statusBarText": {
    "formattingEnabled": "On $(heart-filled)",
    "formattingDisabled": "Off $(heart)"
  },

